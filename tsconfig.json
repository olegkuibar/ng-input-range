{
  "compileOnSave": false,
  "compilerOptions": {
    "incremental": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "strict": true,
    "noImplicitReturns": true,
    "stripInternal": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "moduleResolution": "node",
    "lib": [
      "ES6",
      "dom"
    ],
    "target": "ES6",
    "module": "es2020",
    "paths": {
      "ng-input-range": [
        "dist/ng-input-range/ng-input-range",
        "dist/ng-input-range"
      ]
    }
  },
  "angularCompilerOptions": {
    "strictTemplates": true,
  }
}
